body.user-login-reg{ background-image:url(../img/login-reg-bg-1.jpg);}
body.user-login-reg .top-nav-box{border-bottom:2px solid #33b5e5;}

/*
id="user-reg-box-page" 会员注册
*/
.user-reg-box-page{width:980px; height:700px; display:block; margin:0 auto; background-image:url(../img/reg-box-bg-1.jpg); background-position:right top; background-repeat:no-repeat; background-color:#fff;margin-bottom:74px;margin-top:30px;}
.user-reg-box-page .l{width:530px; height:auto; display:block; float:left;}
.user-reg-box-page .l .reg{ display:block;padding-left:57px;padding-top:58px;}
.user-reg-box-page .l .reg form{padding:0px;margin:0px;}
.user-reg-box-page .l .reg .row-txt{ height:89px;width:100%; display:block;}
.user-reg-box-page .l .reg .row-txt .t1{color:#69b3f2;font-size:26px;font-family:'microsoft yahei';}
.user-reg-box-page .l .reg .row-txt .t2{color:#69b3f2; display:block;padding-top:10px;}

.user-reg-box-page .l .reg .row-1{width:100%; height:97px; display:block;}
.user-reg-box-page .l .reg .row-1 .email-tip{width:392px; height:auto; display:block; padding-top:10px; text-align:right;}
.user-reg-box-page .l .reg .row-1 .email-tip a{color:#406eba; text-decoration:none;}
.user-reg-box-page .l .reg .row-1 .email-tip a:hover{color:#406eba; text-decoration:underline;}
.user-reg-box-page .l .reg .row-2{width:100%; height:84px; display:block;}
.user-reg-box-page .l .reg .row-3{width:100%; height:84px; display:block;}
.user-reg-box-page .l .reg .row-4{width:100%; height:67px; display:block;}

.user-reg-box-page .l .reg .row-1 .set-pwd{display:none;display/*\**/:block \9;color:#ccc;padding-left:5px;}
.user-reg-box-page .l .reg .row-3 .set-pwd{display:none;display/*\**/:block \9;color:#ccc;padding-left:5px;}
.user-reg-box-page .l .reg .row-4 .set-pwd{display:none;display/*\**/:block \9;color:#ccc;padding-left:5px;}

.user-reg-box-page .l .reg .row-5{width:100%; height:67px; display:block;}
.user-reg-box-page .l .reg .row-5 .u-name{width:40px;height:46px; display:block; float:left;font-family:'arial'; font-weight:bold;color:#666; white-space:nowrap;padding-left:5px;}
.user-reg-box-page .l .reg .row-5 .u-sec{width:140px;height:46px; display:block; float:left;}
.user-reg-box-page .l .reg .row-5 .u-sec .r-c{}
.user-reg-box-page .l .reg .row-5 .u-sec .r-c{}
.user-reg-box-page .l .reg .row-5 .u-sec .r-c label{color:#6b6b6b;}
				
.user-reg-box-page .l .reg .row-6{width:100%; height:50px; display:block;padding-top:10px;}
.user-reg-box-page .l .reg .row-6 .btn-reg{width:390px; height:50px;  line-height:50px; color:#fff; display:block; text-align:center;font-family:'microsoft yahei'; background-color:#69b3f2;-webkit-border-radius: 2px;
border-radius: 2px;
-moz-border-radius : 2px;border:0px; cursor:pointer;font-size:28px;}
.user-reg-box-page .l .reg .row-6 .btn-reg:hover{background-color:#59a4e4;}
.user-reg-box-page .l .reg .row-7{width:100%; height:auto; display:block; padding-top:19px;color:#656565;}
.user-reg-box-page .l .reg .row-7 a{color:#69b3f2; text-decoration:none;}
.user-reg-box-page .l .reg .row-7 a:hover{text-decoration:underline;}

.user-reg-box-page .l .reg .row-1 .user_email_bd{width:392px;height:52px; display:block;padding-top:2px;padding-left:2px;}
.user-reg-box-page .l .reg .row-2 .user_name_bd{width:392px;height:52px; display:block;padding-top:2px;padding-left:2px;}
.user-reg-box-page .l .reg .row-3 .user_pwd_bd{width:392px;height:52px; display:block;padding-top:2px;padding-left:2px;} 
.user-reg-box-page .l .reg .row-4 .user_pwd_bd{width:392px;height:52px; display:block;padding-top:2px;padding-left:2px;}
.user-reg-box-page .l .reg .row-1 .cur{background-color:#d9eeff;}
.user-reg-box-page .l .reg .row-2 .cur{background-color:#d9eeff;}
