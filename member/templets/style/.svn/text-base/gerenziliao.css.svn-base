}




.btc8-chongzhi,.btc8-chzhxx,.btc8-chzhxx ul{
	float:left;
	width:100%;
}
.btc8-chzhxx ul li{
	display:inline-block;
	width:auto;
	_float:left;
	_width:80px;
	height:30px;
	
	
}
.btc8-chzhxx ul li a{
	display:block;
	width:auto;
	_width:60px;
	height:30px;
	font:12px/30px 微软雅黑;
	color:#fff;
	padding:0 10px;
	background:#333;
	border:1px solid #ccc;
	border-top-left-radius:6px;
	border-top-right-radius:6px;
	border-bottom:none;
	_text-align:center;
	_margin-right:5px;
}
.btc8-chzhxx ul li a:hover,.btc8-chzhxx ul li a.btc8-chzhxx-avist{
	display:block;	
	height:30px;
	font:12px/30px 微软雅黑;
	color:#333;
	padding:0 10px;
	border:1px solid #ccc;
	background:url(../images/w_r2_c4.jpg) repeat-x;
	border-top-left-radius:6px;
	border-top-right-radius:6px;
	border-bottom:none;
	
}
