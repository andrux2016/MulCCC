.form-line {
	position: relative;
	padding-top: 16px;
	*zoom: 1
}

.form-line:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	height: 0;
	content: ' ';
	clear: both
}

.form-line input,.form-line label {
	width: 226px
}

.form-line label {
	font-size: 14px;
	float: left;
	height: 37px;
	line-height: 37px;
	margin-bottom: -37px;
	overflow: hidden;
	padding: 0 2px 0 10px;
	color: #999
}

.form-line .form-input {
	height: 32px;
	padding: 1px 1px 1px 6px;
	*line-height: 32px;
	border: 1px solid #b6c4cd;
	font-size: 20px;
	letter-spacing: 1px;
	background-color: transparent;
	float: left;
	outline: none
}
.form-line .form-input:focus,.form-line .form-input:Active{
	background:#fff;
}

.form-line a {
	display: inline-block;
	padding: 0 10px
}

.form-line a.lnk-reg {
	padding-left: 0
}

.form-line-action {
	padding-top: 19px
}

.form-line-links {
	color: #cfcfcf;
	padding-top: 22px
}

.form-focus label {
	color: #CCC
}

.form-focus input {
	border-color: #9abcdf
}

.form-msg {
	display: none;
	width: 223px;
	padding: 2px 0 2px 10px;
	margin-top: 5px;
	border: 1px solid #FF7751;
	background-color: #FFF2F2;
	color: #333
}


