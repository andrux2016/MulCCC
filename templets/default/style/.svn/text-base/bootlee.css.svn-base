@charset "utf-8";
a, abbr, acronym, address, applet, article, aside, audio, b, blockquote, big, body, center, canvas, caption, cite, code, command, datalist, dd, del, details, dfn, dl, div, dt, em, embed, fieldset, figcaption, figure, font, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, keygen, label, legend, li, meter, nav, object, ol, output, p, pre, progress, q, s, samp, section, small, span, source, strike, strong, sub, sup, table, tbody, tfoot, thead, th, tr, tdvideo, tt, u, ul, var {
	background:transparent;
	border:0 none;
	font-size:100%;
	margin:0;
	padding:0;
	border:0;
	outline:0;
	vertical-align:top
}
ol, ul {list-style:none}
blockquote, q {quotes:none}
table, table td {padding:0;border:none;border-collapse:collapse}
a{
	color:#f90;
}
a:link,a:visited ,a:hover ,a:active {	
	text-decoration: none;	
}
a:hover{
color:#f60;
}


@font-face
{
font-family: '方正粗倩简体';
src: url('../font/方正粗倩简体.ttf'),
     url('../font/方正粗倩简体.eot'); /* IE9 */
}
 

.clear{
	clear:both;
}
.xbt-headers{
	width:100%;	
	background:#2c2c2c;
}
.xbt-header{
	width:100%;	
	margin:0 auto;
	max-width:1000px;
	min-width:320px;
	_width:1000px;
	background:#2c2c2c;
}
.xbt-navs{
	width:100%;	
	background:url(../images/xbt-navbg.jpg) repeat-x;
}
.xbt-nav{
	width:100%;	
	margin:0 auto;
	max-width:1000px;
	min-width:320px;
	_width:1000px;
	background:url(../images/xbt-navbg.jpg) repeat-x;
}


.xbt-con{
	margin:15px auto;
	max-width:1000px;
	min-width:320px;
	_width:1000px;
}



.xbt-footer{
	width:100%;	
	background:url(../images/xbt-footer-bg.jpg) repeat-x;
}
.xbt-foot{
	margin:0 auto;
	max-width:1000px;
	min-width:320px;
	_width:1000px;
	min-height:80px;
	_height:80px;
	padding-top:20px;
	margin-top:10px;
}