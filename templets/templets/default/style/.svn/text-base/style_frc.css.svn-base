}
table.sf-grid {
	width:100%;
	background:#fff;
	border:1px solid #ccc;
	border-collapse:collapse;
	border-spacing:0;
	table-layout:fixed
}
.sf-grid thead th {
	background:#efefef;
	padding:3px;
	color:#666;
	font-weight:bold;
	border-left:1px solid #ccc;
	border-top:1px solid #999;
	border-bottom:1px solid #ccc
}
.sf-grid tbody td,.sf-grid tbody th {
	background:#fff;
	color:#666;
	padding:5px 2px;
	border:1px solid #ddd
}
.sf-grid tfoot td,.sf-grid tfoot th {
	background-color:#efefef;
	color:#666;
	padding:5px 2px
}
.sf-grid th.spec {
	border-left:1px solid #c1dad7;
	border-top:0
}
.sf-grid th.specalt {
	border-left:1px solid #c1dad7;
	border-top:0;
	color:#b4aa9d
}
.sf-grid tbody tr.alt td {
	background:#f5fafa;
	color:#b4aa9d
}
.sf-grid tbody tr.win td {
	background:#fff7d2;
	color:#333
}
.sf-grid tbody tr:hover td,.sf-grid tbody tr:hover th {
	background:#fff7d2;
	color:#333
}
.sf-grid th,.sf-grid td {
	font-size:14px
}
.dropmenu {
	font-family:"Droid Sans",Trebuchet MS,Tahoma,Arial,sans-serif,serif;
	font-size:15px;
	font-style:normal;
	font-weight:400;
	letter-spacing:0;
	text-decoration:none;
	text-shadow:none;
	text-transform:uppercase;
	word-spacing:0;
	font-weight:bold;
	z-index:1;
	margin:0;
	background:none repeat scroll 0 0 #414141;
	width:100%;
	padding:0
}
.dropmenu ul {
	padding:0;
	margin:0;
	list-style-type:none
}
.dropmenu ul li {
	float:left;
	position:relative
}
.dropmenu ul li a,.menu ul li a:visited {
	display:block;
	text-align:center;
	text-decoration:none;
	width:130px;
	height:30px;
	color:#e4e4e4;
	border:0 solid #fff;
	border-width:0;
	background:#414141;
	line-height:30px;
	font-size:15px
}

.dropmenu ul li a.show {
	background: #FF6600;
	color:#FFF
} 

.dropmenu ul li ul {
	display:none
}
.dropmenu ul li:hover a {
	color:#fff;
	background:#315c98
}
.dropmenu ul li:hover ul {
	display:block;
	position:absolute;
	top:30px;
	left:0;
	width:105px;
	z-index:1
}
.dropmenu ul li:hover ul li a {
	display:block;
	background:#414141;
	color:#e4e4e4
}
.dropmenu ul li:hover ul li a:hover {
	background:#315c98;
	color:#fff
}
.btn {
	display:inline;
	zoom:1;
	margin-bottom:0;
	font-size:14px;
	line-height:15px;
	text-align:center;
	vertical-align:middle;
	cursor:pointer;
	color:#333;
	text-shadow:0 1px 1px rgba(255,255,255,0.75);
	background-image:linear-gradient(tobottom,#fff,#e6e6e6);
	background-repeat:repeat-x;
	background-color:#e6e6e6;
	filter:progid:DXImageTransform.Microsoft.gradient(enabled=false);
	border:0;
	margin-right:5px;
	-webkit-box-shadow:inset 0 1px 0 rgba(255,255,255,.2),0 1px 2px rgba(0,0,0,.05);
	-moz-box-shadow:inset 0 1px 0 rgba(255,255,255,.2),0 1px 2px rgba(0,0,0,.05);
	box-shadow:inset 0 1px 0 rgba(255,255,255,.2),0 1px 2px rgba(0,0,0,.05);
	border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) #b3b3b3;
	padding:4px 12px
}
.btn-success {
	color:#fff;
	text-shadow:0 -1px 0 rgba(0,0,0,0.25);
	background-image:linear-gradient(tobottom,#62c462,#51a351);
	background-repeat:repeat-x;
	background-color:#39c;
	filter:progid:DXImageTransform.Microsoft.gradient(enabled=false);
	border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25)
}
.btn-group {
	position:relative;
