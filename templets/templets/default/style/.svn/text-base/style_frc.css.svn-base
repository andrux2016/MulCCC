* {
	margin:0;
	padding:0
}
header,nav,section,article,aside_left,aside_right,footer {
	display:block
}
body {
	font:14px/18px "Trebuchet MS",Tahoma,Arial,sans-serif;
	color:#444;
	background:#ededed url(../images/bodybg.png) repeat-x
}
::-moz-selection {
