.deallist {
  width: 710px;
  position: relative;
  margin-left:auto;
  margin-right:auto;
}

.deallist ul li {
  display:block;
  color:#6C6D61;
  float:left;
  /*padding: 10px 6px;*/
  border-right:1px solid #c0c0c0;
  border-bottom:1px solid #c0c0c0;
  height: 22px;
  text-align:center;
}
.order {
  width: 345px;
  position: relative;
  margin-left:auto;
  margin-right:auto;
}
.order ul li {
  display:block;

  float:left;
  border-right:1px solid #DADADA;
  border-bottom:1px solid #DADADA;
  height: 22px;
  text-align:center;
}

.orderlist {
  width: 100%; 
  max-height:345px;
  height:expression( this.scrollHeight > 337 ? "337px":"auto" );
  overflow-y:auto
  /*height: 350px;*/
  /*margin: 0 10px;*/
}
.orderlist ul {
  width: 100%; 
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none;

}

.userOrder {
  width: 100%;
  position: relative;
  margin-left:auto;
  margin-right:auto;
  max-height:345px;
  height:expression( this.scrollHeight > 337 ? "337px":"auto" );
  overflow-y:auto
}
.userOrder ul {
  width: 97%; 
  list-style-image:none;
  list-style-position:outside;
  list-style-type:none;

}

.userOrder ul span {
  display:block;
  color:#6C6D61;
  float:left;
  border-right:1px solid #DADADA;
  border-bottom:1px solid #DADADA;
  height: 22px;
  text-align:center;
}
.clear{
	overflow:hidden;
}
